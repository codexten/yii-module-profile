{
  "name": "codexten/yii-module-profile",
  "type": "library",
  "description": "Profile module for yii 2.0 or higher",
  "require": {
    "codexten/yii-module-auth": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\modules\\profile\\": "src/modules/profile"
    }
  },
  "suggest": {
    "codexten/yii-sms": "for sending SMS"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "sconfig-plugin": {
      "i18n": "config/i18n.php",
      "adminSettings": "config/adminSettings.php",
      "moduleAuth": [
        "$web",
        "config/moduleAuth.php"
      ],
      "migrationNamespaces": "config/migrationNamespaces.php"
    }
  }
}
